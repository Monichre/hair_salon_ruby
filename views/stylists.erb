<% if @stylists.any? %>
  <h2>Browse our current stylists</h2>
  <ul>
    <% @stylists.each do |stylist| %>
    <li><a href="/stylists/<%=stylist.id() %>"><%= stylist.name() %></a></li>
    <% end %>
  </ul>
<% else %>

  <p>There are no stylists at this time. Please contact our receptionist, thank you</p>
  <li><span>>555-555-5555</span></li>
<% end %>
